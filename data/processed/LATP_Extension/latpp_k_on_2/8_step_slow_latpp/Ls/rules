Rule('RLs_bind', R() + Ls() | RLs(), k_L_on, k_Ls_off) 
Rule('RLs_Lck_bind', RLs() + Lck() | RLs_Lck(), k_lck_on_RL, k_lck_off_RL) 
Rule('RLs_Lck_unbindL', RLs_Lck() | Ls() + R_Lck(), k_Ls_off, k_L_on) 
Rule('R_Lck_unbindLck', R_Lck() | R() + Lck(), k_lck_off_R, k_lck_on_R) 
Rule('RPLs_Lck_cat', RLs_Lck() | RPLs_Lck(), k_p_on_R_pmhc, k_p_off_R_pmhc) 
Rule('RPLs_Lck_unbindL', RPLs_Lck() | Ls() + RP_Lck(), k_Ls_off, k_L_on) 
Rule('RP_Lck_unbindLck', RP_Lck() | RP() + Lck(), k_lck_off_R, k_lck_on_R) 
Rule('RP_uncat', RP() | R(), k_p_off_R, k_p_on_R) 
Rule('RPLs_Lck_Zap_bind', RPLs_Lck() + Zap() | RPLs_Lck_Zap(), k_zap_on_R_pmhc, k_zap_off_R_pmhc) 
Rule('RPLs_Lck_Zap_unbindL', RPLs_Lck_Zap() | Ls() + RP_Lck_Zap(), k_Ls_off, k_L_on) 
Rule('RP_Lck_Zap_unbindLck', RP_Lck_Zap() | RP_Zap() + Lck(), k_lck_off_zap_R, k_lck_on_R) 
Rule('RP_Zap_unbindZap', RP_Zap() | RP() + Zap(), k_zap_off_R, k_zap_on_R) 
Rule('RPLs_Lck_Zap_P_cat', RPLs_Lck_Zap() | RPLs_Lck_Zap_P(), k_p_on_zap_species, k_p_off_zap_species) 
Rule('RPLs_Lck_Zap_P_unbindL', RPLs_Lck_Zap_P() | Ls() + RP_Lck_Zap_P(), k_Ls_off, k_L_on) 
Rule('RP_Lck_Zap_P_unbindLck', RP_Lck_Zap_P() | RP_Zap_P() + Lck(), k_lck_off_zap_R, k_lck_on_R) 
Rule('RP_Zap_P_uncat', RP_Zap_P() | RP_Zap(), k_p_off_R, k_p_on_R) 
Rule('RPLs_Lck_Zap_P_LAT_bind', RPLs_Lck_Zap_P() + LAT() | RPLs_Lck_Zap_P_LAT(), k_lat_on_species, k_lat_off_species) 
Rule('RPLs_Lck_Zap_P_LAT_unbindL', RPLs_Lck_Zap_P_LAT() | Ls() + RP_Lck_Zap_P_LAT(), k_Ls_off, k_L_on) 
Rule('RP_Lck_Zap_P_LAT_unbindLck', RP_Lck_Zap_P_LAT() | RP_Zap_P_LAT() + Lck(), k_lck_off_zap_R, k_lck_on_R) 
Rule('RP_Zap_P_LAT_uncat', RP_Zap_P_LAT() | RP_Zap_LAT(), k_p_off_R, k_p_on_R) 
Rule('RP_Zap_LAT_rpzap_off', RP_Zap_LAT() | RP_Zap() + LAT(), k_zap_off_R, k_zap_on_R) 
Rule('RPLs_Lck_Zap_P_LATP_bind', RPLs_Lck_Zap_P_LAT() | RPLs_Lck_Zap_P_LATP(), kp_on_lat1, kp_off_lat1) 
Rule('RPLs_Lck_Zap_P_LATP_unbindL', RPLs_Lck_Zap_P_LATP() | Ls() + RP_Lck_Zap_P_LATP(), k_Ls_off, k_L_on) 
Rule('RP_Lck_Zap_P_LATP_unbindLck', RP_Lck_Zap_P_LATP() | RP_Zap_P_LATP() + Lck(), k_lck_off_zap_R, k_lck_on_R) 
Rule('RP_Zap_P_LATP_uncat', RP_Zap_P_LATP() | RP_Zap_LATP(), k_p_off_R, k_p_on_R) 
Rule('RP_Zap_LATP_uncat', RP_Zap_LATP() | RP_Zap_LAT(), k_p_off_R, k_p_on_R) 
Rule('RPLs_Lck_Zap_P_LATPP_bind', RPLs_Lck_Zap_P_LATP() | RPLs_Lck_Zap_P_LATPP(), kp_on_lat2, kp_off_lat2) 
Rule('RPLs_Lck_Zap_P_LATPP_unbindL', RPLs_Lck_Zap_P_LATPP() | Ls() + RP_Lck_Zap_P_LATPP(), k_Ls_off, k_L_on) 
