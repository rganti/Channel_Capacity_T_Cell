Rule('Ras_GAP_Ras_GTP_bind', Ras_GAP() + Ras_GTP() | Ras_GAP_Ras_GTP(), k_rgap_on_rgtp, k_rgap_off_rgtp) 
Rule('Ras_GAP_Ras_GTP_cat', Ras_GAP_Ras_GTP() >> Ras_GAP() + Ras_GDP(), k_cat_5) 
